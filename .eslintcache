   }\n    }\n\n    setModelData(render)\n    addData(d);\n  }\n\n  const handleOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div className=\"container\">\n        {modelData.map(data => (\n          data.id === 12 ? \n          <div onClick={() => choose(data.id)} className='freeSlot'>\n            <img src=\"https://www.playbingoaustralia.com/wp-content/uploads/2015/02/Bingo-Night.png\" alt=\"freeImage\" width=\"100%\"></img>\n          </div> : \n          <div onClick={() => choose(data.id)} className={data.selected ? 'itemSelected' : 'item'}>\n            <div className=\"number\">\n              <p>{data.id}</p>\n            </div>\n            <div className=\"text\">\n              <p>{data.item}</p>\n            </div>\n          </div>\n      ))}\n      <Modal\n        aria-labelledby=\"transition-modal-title\"\n        aria-describedby=\"transition-modal-description\"\n        className={classes.modal}\n        open={open}\n        onClose={handleClose}\n        closeAfterTransition\n        BackdropComponent={Backdrop}\n        BackdropProps={{\n          timeout: 500,\n        }}\n      >\n        <Fade in={open}>\n          <div className={classes.paper}>\n            <img src=\"https://media.giphy.com/media/1wCjfLiSZ2FWhUc2vu/giphy.gif\" alt=\"funny GIF\" width=\"100%\" height='420px'/>\n          </div>\n        </Fade>\n      </Modal>\n    </div>\n  );\n}\n\n\nexport default Home;\n","/Users/bekirinan/Downloads/bingo-app/src/index.js",[],"/Users/bekirinan/Downloads/bingo-app/src/screens/Model/BingoModel.js",[],"/Users/bekirinan/Downloads/bingo-app/src/styles/bingoStyles/homeStyles.js",[],{"ruleId":"30","severity":1,"message":"31","line":16,"column":31,"nodeType":"32","messageId":"33","endLine":16,"endColumn":53},{"ruleId":"30","severity":1,"message":"34","line":17,"column":31,"nodeType":"32","messageId":"33","endLine":17,"endColumn":53},{"ruleId":"35","severity":1,"message":"3